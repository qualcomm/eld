RUN: %clang %clangopts -target arm -c %p/Inputs/1.c -o %t1.o
RUN: %link %linkopts -march arm %t1.o  -o %t.out --section-start=.my_start=0x1000 -T=%p/Inputs/script.t 2>&1
RUN: %readelf -s %t.out  | %filecheck %s
RUN: %not %link %linkopts -march arm %t1.o  -o %t.out --section-start=.my_start=0x1000 -T=%p/Inputs/errorexpn.t 2>&1 | %filecheck -check-prefix=ERR %s
RUN: %link %linkopts -march arm %t1.o  -o %t.out --section-start=.my_start=0x1000 -T=%p/Inputs/addr.t 2>&1
RUN: %readelf -s %t.out | %filecheck --check-prefix=ADDR %s
RUN: %not %link %linkopts -march arm %t1.o  -o %t.out --section-start=.my_start=0x1000 -T=%p/Inputs/addr_error.t 2>&1 | %filecheck -check-prefix=ERR2 %s

