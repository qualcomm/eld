#---InitializeGOTPLT0DynamicZNow.test----------------------- SharedLibrary --------------------#
#BEGIN_COMMENT
# This tests that the linker sets the first slot in GOTPLT0 to zero.
#END_COMMENT
#START_TEST
RUN: %clang %clangopts -c %p/Inputs/1.c -o %t1.1.o %clangg0opts -fPIC
RUN: %link %linkopts -z now -shared %t1.1.o -o %t1.lib1.so -z max-page-size=0x1000
RUN: %readelf -x .got %t1.lib1.so | %filecheck %s

#CHECK: {{[0x0]+}} {{[0x0]+}} {{[0x0]+}}
#END_TEST
