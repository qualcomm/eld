#ATTRS:         Tag: 4
#ATTRS:         Value: 16
#ATTRS:         TagName: stack_align
#ATTRS:         Description: Stack alignment is 16-bytes
#ATTRS:       }
#ATTRS:       Attribute {
#ATTRS:         Tag: 6
#ATTRS:         Value: 1
#ATTRS:         TagName: unaligned_access
#ATTRS:         Description: Unaligned access
#ATTRS:       }
#ATTRS:       Attribute {
