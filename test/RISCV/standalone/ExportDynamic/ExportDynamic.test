#BEGIN_COMMENT
# Enable export dynamic switch to work on RISC-V
#--------------------------------------------------------------------
#END_COMMENT
#
RUN: %clang %clangopts -c %p/Inputs/1.c -o %t1.1.o -flto
RUN: %link %linkopts %t1.1.o --export-dynamic -o %t2.out
RUN: %readelf -s %t2.out | %filecheck %s

