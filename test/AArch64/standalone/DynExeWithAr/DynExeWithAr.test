
%clang %clangopts -c fn.c fn1.c -fPIC 1.c
RUN: %clang %clangopts -target aarch64 -c %p/Inputs/1.c -o %t1.o -fPIC
RUN: %clang %clangopts -target aarch64 -c %p/Inputs/fn1.c -o %t2.o -fPIC
RUN: %clang %clangopts -target aarch64 -c %p/Inputs/fn.c -o %t3.o -fPIC
RUN: rm -rf %t4
RUN: mkdir %t4
RUN: %ar cr %aropts %t4/libfn.a %t3.o
RUN: %ar cr %aropts %t4/libfn1.a %t2.o
RUN: %link %linkopts -march aarch64 -dy -Bdynamic %t1.o -L%t4 --start-group -lfn -lfn1 --end-group --force-dynamic -o %t.out
RUN: %readelf -s %t.out | %filecheck %s

CHECK: fn1
