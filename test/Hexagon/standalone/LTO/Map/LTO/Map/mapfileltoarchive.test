# YAML: - Name:            .text
# YAML: - Symbol:          main
# YAML: - Name:            .text.foo
# YAML: - Symbol:          foo
# YAML: - Name:            .bss
# YAML: - Name:            COMMON.common_array
# YAML: - Symbol:          common_array
