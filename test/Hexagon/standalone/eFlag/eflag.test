RUN: %link %linkopts -r -mcpu hexagonv71 %t1.o -o %t1.elf.mv71.2.r
RUN: %link %linkopts %t2.o %t1.elf.mv71.2.r -o %t1.elf.mv71.3

# Generate V71t objects
