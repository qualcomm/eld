#---InitializeGOTPLT0.test----------------------- SharedLibrary/Executable --------------------#

#BEGIN_COMMENT
# This tests that the linker sets the first slot in GOTPLT0 to zero.
#END_COMMENT
#START_TEST
RUN: %clang %clangopts -c %p/Inputs/1.c -o %t1.1.o %clangg0opts -fPIC
RUN: %link %linkopts -shared %t1.1.o -o %t1.lib1.so
RUN: %readelf -x .got.plt %t1.lib1.so | %filecheck %s
RUN: %link %linkopts -shared %t1.1.o -o %t1.out
RUN: %readelf -x .got.plt %t1.out | %filecheck %s

#CHECK: {{[0x0]+}} {{[0x0]+}} {{[0x0]+}} {{[0x0]+}}
#END_TEST
