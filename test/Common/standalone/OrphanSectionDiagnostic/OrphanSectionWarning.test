#---OrphanSectionsWarning.test------------Executable --------------------#
#BEGIN_COMMENT
# This test checks if a diagnostic warning is emitted when an orphan section
# is detected when orphan_handling is set to "warn".
#END_COMMENT
#START_TEST
RUN: %clang %clangopts -c %p/Inputs/1.c -o %t1.1.o -ffunction-sections
RUN: %link %linkopts -orphan-handling=warn %t1.1.o -T %p/Inputs/script.t -o %t2.out 2>&1 | %filecheck %s

#CHECK:Warning: no linker script rule for section .text.bar