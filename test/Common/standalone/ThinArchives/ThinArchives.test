#---ThinArchives.test--------------------- Executable---------------------#
#BEGIN_COMMENT
# This test checks the basic thin archive support.
#END_COMMENT
#START_TEST

RUN: %rm -rf %t.tmpdir
RUN: mkdir -p %t.tmpdir

RUN: %clang %clangopts -o %t.tmpdir/1.o %p/Inputs/1.c -c
RUN: %clang %clangopts -o %t.tmpdir/2.o %p/Inputs/2.c -c

RUN: %ar cr %aropts --thin %t.tmpdir/libthin.a %t.tmpdir/1.o %t.tmpdir/2.o

RUN: %clang %clangopts -o %t.tmpdir/main.o %p/Inputs/main.c -c
RUN: %link -MapStyle txt %linkopts -o %t.tmpdir/main.out %t.tmpdir/main.o %t.tmpdir/libthin.a -Map %t.tmpdir/map.txt

RUN: %readelf -s %t.tmpdir/main.out | %filecheck %s --check-prefix READELF

RUN: %filecheck %s --check-prefix MAP < %t.tmpdir/map.txt

RUN: %link -MapStyle txt %linkopts -o %t.tmpdir/main.out %t.tmpdir/main.o %t.tmpdir/libthin.a -Map %t.tmpdir/map.relative.txt --MapDetail relative-path=%t.tmpdir

RUN: %filecheck %s --check-prefix RELATIVE_MAP < %t.tmpdir/map.relative.txt
#END_TEST

READELF-DAG: main
READELF-DAG: foo
READELF-DAG: anotherFoo
READELF-DAG: bar
READELF-DAG: anotherBar

MAP: {{.*}}libthin.a({{.*}}/1.o)
MAP: foo
MAP : anotherFoo
MAP: {{.*}}libthin.a({{.*}}/2.o)
MAP: bar
MAP: anotherBar

RELATIVE_MAP: libthin.a(1.o)
RELATIVE_MAP: foo
RELATIVE_MAP : anotherFoo
RELATIVE_MAP: libthin.a(2.o)
RELATIVE_MAP: bar
RELATIVE_MAP: anotherBar
